{
    "name": "cxl/blocks",
    "type": "wordpress-plugin",
    "description": "Blocks for CXL Projects, such as Content Layout etc.",
    "authors": [
        {
            "name": "Leho Kraav",
            "email": "leho@conversionxl.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/conversionxl/phpcs-ruleset"
        }
    ],
    "require": {
        "php" : ">=7.4"
    },
    "require-dev": {
        "cxl/phpcs-ruleset": "dev-master",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "CXL\\Blocks\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
